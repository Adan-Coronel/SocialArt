{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/albums/.",
            "dest": "server/index.js"
        },
        {
            "src": "/perfil/.",
            "dest": "server/index.js"
        },
        {
            "src": "/imagenes/.",
            "dest": "server/index.js"
        },
        {
            "src": "/registro",
            "dest": "server/index.js"
        },
        {
            "src": "/login",
            "dest": "server/index.js"
        },
        {
            "src": "/logout",
            "dest": "server/index.js"
        },
        {
            "src": "/",
            "dest": "server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}